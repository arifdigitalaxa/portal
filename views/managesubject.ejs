<div id='main' ng-controller="AdminSubjectController" ng-init ="getAllSubject()">
    <div class="container" ng-cloak> 
	    <div class="row col-md-10 col-md-offset-1 custyle">
	    	<h3 class="text-center">Subject List</h3> 
	        <table class="table table-striped custab">
	        	<thead>
	        	<a href="#" class="btn btn-primary btn-xs pull-right" ng-click="addSubject()"><b>+</b> Add new subject</a>
	        		<tr>
	        			<th>Subject</th>
	        			<th>Description</th>
	        			<!-- <th>Edit</th>
 -->	        		<th class="text-center">Delete</th>
 						<th class="text-center">Add Question</th>
	        		</tr>
	        	</thead>
	        	<tbody>
	        		<tr ng-repeat="subject in subjectList track by subject.id">
						<th>{{subject.name}}</th>
	        			<th>{{subject.desc}}</th>
	        			<!-- <th><i class="fa fa-pencil-square-o text-primary fa-lg" aria-hidden="true" ng-click=""></i></th> -->
	        			<th class="text-center"><i class="fa fa-trash text-danger fa-lg" aria-hidden="true" ng-click="deleteSubject(ev,subject)"></i></th>
	        			<th class="text-center"><strong><i class="fa fa-plus-square text-success fa-lg" aria-hidden="true" ng-click="addQuestion(ev,subject)"></i></strong></th>
	        		</tr>
	        	</tbody>
	        </table>
	    </div>
    </div>  
</div>
